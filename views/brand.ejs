<%- include('partials/header') %>
<div class="container mt-5">
  <form method="POST" action="/brand">
    <div class="row">
      <div class="col-sm-3">
        <label for="exampleInputBrand1" class="form-label">Brand</label>
        <input
          type="brand"
          name="brand"
          class="form-control"
          id="exampleInputBrand1"
          aria-describedby="brandHelp"
          placeholder="enter brand"
        />
      </div>
      <div class="col-sm-3">
        <label for="exampleInputModel1" class="form-label">Model</label>
        <input
          type="model"
          name="model"
          class="form-control"
          id="exampleInputModel1"
          placeholder="enter model"
        />
      </div>
      <div class="col-sm-3">
        <label for="exampleInputVariant1" class="form-label">Variant</label>
        <input
          type="variant"
          name="variant"
          class="form-control"
          id="exampleInputVariant1"
          placeholder="enter variant"
        />
      </div>
      <div class="col-sm-2">
        <label class="form-label"></label>
        <button type="submit" class="btn btn-primary">ADD</button>
      </div>
    </div>
  </form>
</div>
<%- include('partials/header') %>
